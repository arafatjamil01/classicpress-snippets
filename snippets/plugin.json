{
	"Plugin_start": {
		"prefix": "start_plugin",
		"body": [
			"/**",
			" * Plugin Name: ${1:Plugin Name}",
			" * Plugin URI: ${2:Plugin Author Link}",
			" * Author Name: ${3:Plugin Author Name}",
			" * Author URI: ${4:Plugin Author Link}",
			" * Description: ${5:This plugin does wonders}",
			" * Version: ${6:0.1.0}",
			" * License: ${6:GPL-2.0+}",
			" * License URL: ${7:http://www.gnu.org/licenses/gpl-2.0.txt}",
			" * text-domain: ${8:prefix-plugin-name}",
			"*/"
		],
		"description": "Plugin start up details bootstrap"
	},
	"plugin_esc": {
		"prefix": "plugin_esc",
		"body": [
			"// If this file is called directly, abort!",
			"defined( 'ABSPATH' ) or die( 'Unauthorized Access!' );"
		],
		"description": "Plugin escape if this file is called directly, abort!"
	},
	"var_dump_pre_custom": {
		"prefix": "var_dump_pre_custom",
		"body": [
			"echo '<pre>';",
			"var_dump( wp_remote_retrieve_retrieve_body( $response ) );",
			"echo '</pre>';"
		],
		"description": "Custom var_dump"
	}
}
